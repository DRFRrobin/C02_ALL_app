{
  "name": "mywebdesktop",
  "version": "1.0.0",
  "description": "Windows-like desktop environment",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "pack": "electron-packager ./ MyWebDesktop --platform=win32 --arch=x64 --out ../dist --overwrite --prune=true"
